{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The Root Schema",
    "description": "The root schema comprises the entire JSON document.",
    "required": [
        "id",
        "created_at",
        "modified_at",
        "registration_number",
        "name",
        "position",
        "raw_salary",
        "admission_date",
        "department"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "Object ID",
            "description": "ID.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "Creation date of object",
            "description": "Creation date of object in ISO Format",
            "default": "",
            "examples": [
                "2020-04-01T03:44:26.542343Z"
            ]
        },
        "modified_at": {
            "$id": "#/properties/modified_at",
            "type": "string",
            "title": "Latest modification of object",
            "description": "Latest modification of object",
            "default": "",
            "examples": [
                "2020-04-01T03:44:26.542376Z"
            ]
        },
        "registration_number": {
            "$id": "#/properties/registration_number",
            "type": "string",
            "title": "Registration of employee",
            "description": "Registration of employee",
            "default": "",
            "examples": [
                "0009968"
            ]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "Employee name",
            "description": "Employee name",
            "default": "",
            "examples": [
                "Victor Wilson"
            ]
        },
        "position": {
            "$id": "#/properties/position",
            "type": "string",
            "title": "Employee Position",
            "description": "Employee Position.",
            "default": "",
            "examples": [
                "Diretor Financeiro"
            ]
        },
        "raw_salary": {
            "$id": "#/properties/raw_salary",
            "type": "string",
            "title": "Employee Salary",
            "description": "Employee Salary.",
            "default": "",
            "examples": [
                "12696.20"
            ]
        },
        "admission_date": {
            "$id": "#/properties/admission_date",
            "type": "string",
            "title": "Employee Admission Date",
            "description": "Employee Admission Date.",
            "default": "",
            "examples": [
                "2012-01-05"
            ]
        },
        "department": {
            "$id": "#/properties/department",
            "type": "integer",
            "title": "The department object id",
            "description": "The department object id.",
            "default": 0,
            "examples": [
                1
            ]
        }
    }
}
