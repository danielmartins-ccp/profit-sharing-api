{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The Root Schema",
    "description": "Department schema definition.",
    "required": [
        "id",
        "created_at",
        "modified_at",
        "name"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "Identification of object",
            "description": "ID.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "Creation date of object",
            "description": "Creation date of object in ISO Format",
            "default": "",
            "examples": [
                "2020-04-01T03:44:26.542343Z"
            ]
        },
        "modified_at": {
            "$id": "#/properties/modified_at",
            "type": "string",
            "title": "Latest modification of object",
            "description": "Latest modification of object",
            "default": "",
            "examples": [
                "2020-04-01T03:44:26.542376Z"
            ]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "Name of department",
            "description": "Name of department.",
            "default": "",
            "examples": [
                "Diretoria"
            ]
        }
    }
}
